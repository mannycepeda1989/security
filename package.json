    "@mapbox/geo-lang-id": "2.1.0",
    "@mapbox/geojsonhint": "^3.0.0",
    "@mapbox/hash-key": "^1.1.0",
    "@mapbox/mapbox-error": "^3.0.1",
    "@mapbox/polyline": "^1.1.0",
