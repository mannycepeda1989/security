    "through2": "^3.0.1",
    "uuid": "^3.4.0",
    "xregexp": "^4.3.0"
  },
  "scripts": {
    "start": "node server.js",
    "json-lint": "find . -not -path './node_modules/*' -not -path '*/fixtures/*' -name '*.json' -exec echo {} \\; -exec jsonlint {} -q \\;",
    "lint": "eslint --fix *.js 'test/**/*.js' 'lib/**/*.js' 'indexer/**/*.js' 'scripts/**/*.js' 'utils/**/*.js' 'cloudformation/**.js' 'autoscaling/**/*.js' 'cluster_proxy/**/*.js'",
    "devserver": "node test/scripts/devserver.js",
    "test": "npm run lint && npm run json-lint  && npm run unit-test",
    "coverage": "nyc report --reporter html && opener coverage/index.html",
    "unit-test": "nyc tape test/unit/*.test.js  test/unit/middleware/*.test.js --coverage",
    "integration-test": "node test/scripts/run-integration-tests.js",
    "integration-jp-test": "node test/scripts/run-integration-jp-tests.js",
    "integration-test-dev": "nyc tape test/integration/*.test.js test/integration/**/*.test.js --coverage",
    "quality-test": "node test/scripts/run-quality-tests.js",
    "cluster-proxy": "node cluster_proxy/index.js",
    "regression-test": "tape test/regression/regression.test.js",
    "restore-snapshot-to-cluster": "node test/scripts/restore-snapshot-to-cluster.js",
    "setup-resources": "aws s3 sync s3://mapbox-search-analysis/resources resources"
  },
  "name": "api-poi-search",
  "description": "Dedicated POI Search API",
  "version": "1.2.0",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "eslintConfig": {
    "extends": [
      "@mapbox/eslint-config-mapbox",
      "@mapbox/eslint-config-mapbox/promise"
    ],
    "plugins": [
      "json"
    ],
    "rules": {
      "space-before-blocks": [
        "error",
        "always"
      ],
      "arrow-spacing": [
        "error"
      ],
      "no-prototype-builtins": "off"
    }
  },
  "devDependencies": {
    "@mapbox/api-poi-search-quality-test": "1.5.6",
    "@mapbox/eslint-config-mapbox": "^2.0.1",
    "@mapbox/mbxcli": "^4.0.0",
    "@mapbox/mock-aws-sdk-js": "^1.0.0",
    "@mapbox/secret-cloudfriend": "^5.6.1",
    "aws-es-kibana": "^1.0.8",
    "commander": "^7.2.0",
    "dynalite": "^3.1.5",
    "eslint": "^6.8.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jsonlint": "^1.6.3",
    "minimist": "^1.2.5",
    "mock-req": "^0.2.0",
    "mock-res": "^0.5.0",
    "node-mocks-http": "^1.8.1",
    "nyc": "^14.1.1",
    "opener": "^1.5.1",
    "portfinder": "^1.0.25",
    "sinon": "^7.5.0",
    "sinon-express-mock": "^2.2.1",
    "tape": "^4.11.0",
    "tape-await": "^0.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/api-poi-search.git"
  },
  "author": "Mapbox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapbox/api-poi-search/issues"
  },
  "homepage": "https://github.com/mapbox/api-poi-search#readme",
  "engines": {
    "node": ">=18.16.0"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "test/**",
      "coverage/**",
      "cloudformation/**",
      "demo/**",
      "scripts/**",
      "bin/**"
    ]
  }
}
