{
  "dependencies": {
    "@elastic/elasticsearch": "7.6.1",
    "@mapbox/api-poi-search-config": "6.22.0",
    "@mapbox/aws-es-connector": "0.2.0",
    "@mapbox/aws-partitions": "^0.0.8",
    "@mapbox/cfn-config": "^2.22.0",
    "@mapbox/cloudfriend": "6.0.0",
